		"@types/node": "^22.10.1",
		"sirv": "^3.0.0",
		"sirv-cli": "^3.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"@sveltejs/adapter-node": "^5.2.9",
		"@types/js-yaml": "^4.0.9",
		"js-yaml": "^4.1.0",
		"svelte-collapsible": "^0.3.1"
